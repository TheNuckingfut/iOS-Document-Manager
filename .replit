modules = ["swift"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "iOS Document Manager"

[[workflows.workflow]]
name = "iOS Document Manager"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "xcrun simctl bootstatus; swift run DocumentManager"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "xcrun simctl bootstatus; swift run DocumentManager"]
